{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch PBIXAnalyzer",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/bin/Debug/net8.0/PBIXAnalyzer.dll",
